package com.gianproco.makereport;

public class Main {

	public static void main(String[] args) {
		
		Report report = new Report();
		report.setNameFile("Report");
		report.setExtension("txt");
		String content = "";
		for(int i = 0; i < 5; i++) {
			content += "Report: " + i;
		}
		
		report.setContent(content);
		report.makeReport();
	}

}
